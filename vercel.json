{
  "version": 2,
  "builds": [
    {
      "src": "index.js",
      "use": "@vercel/node"
    },
    {
      "src": "api/extract-product.js",
      "use": "@vercel/node"
    }
  ],
  "routes": [
    {
      "src": "/api/product-scraper/extract-from-url",
      "dest": "/api/extract-product.js"
    },    {
      "src": "/(.*)",
      "dest": "/index.js"
    }
  ]
}